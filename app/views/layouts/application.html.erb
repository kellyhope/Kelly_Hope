<!DOCTYPE html>
<html>
  <head>
    <title>KellyHope :: <%= @title %></title>
    <%= stylesheet_link_tag :all %>
    <%= javascript_include_tag :defaults %>
    <%= csrf_meta_tag %>

    <%= stylesheet_link_tag 'main', :media => 'screen' %>

    <!--[if lt IE 9]>
    <script src="http://html5shiv.googlecode.com/svn/trunk/html5.js"></script>
    <![endif]-->
  </head>
  <body>
    <div id="container">
      <div id="header">
        <div class="round" id="logo">
          <%= link_to 'logo goes here', root_path %>
        </div>
        <div class="round" id="nav">
            <p><%= link_to 'About', about_path%> | <%= link_to 'Projects', projects_path%></p>
        </div>
      </div>
      <div class="round" id="content">
        <%= yield %>
      </div>

  </body>
</html>
